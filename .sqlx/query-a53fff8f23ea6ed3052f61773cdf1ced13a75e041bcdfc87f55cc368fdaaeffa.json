{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                w.run_id,\n                w.namespace_id,\n                w.external_id,\n                w.task_queue,\n                w.workflow_type,\n                w.status,\n                p.input,\n                p.output,\n                w.locked_by,\n                w.attempts,\n                w.error,\n                w.created_at,\n                w.started_at,\n                w.finished_at,\n                w.available_at,\n                w.version,\n                w.parent_step_attempt_id,\n                w.retry_policy,\n                w.cron_expr,\n                w.schedule_id\n            FROM kagzi.workflow_runs w\n            JOIN kagzi.workflow_payloads p ON w.run_id = p.run_id\n            WHERE w.namespace_id = $1\n              AND w.status = 'SCHEDULED'\n              AND w.available_at <= $2\n            ORDER BY w.available_at ASC\n            LIMIT $3\n            ",
